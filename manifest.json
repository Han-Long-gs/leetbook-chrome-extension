{
    "name": "LeetBook",
    "description": "Keep track of all questions with failed submissions on LeetCode",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https//leetcode.com/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "./images/icon.png"
    }

}